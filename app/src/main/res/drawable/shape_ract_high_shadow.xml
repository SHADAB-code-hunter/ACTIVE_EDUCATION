<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- disable -->
    <item android:state_enabled="false">
        <layer-list>
            <item>
                <shape >
                    <corners android:radius="5dp"/>
                    <solid android:color="#44292929"/>
                </shape>
            </item>
            <item
                android:bottom="1px"
                android:gravity="center"
                android:left="1px"
                android:right="1px"
                android:top="1px">
                <shape >
                    <corners android:radius="5dp"/>
                    <solid android:color="#292929"/>
                </shape>
            </item>
            <item
                android:bottom="1px"
                android:gravity="center"
                android:left="1px"
                android:right="1px"
                android:top="1px">
                <shape >
                    <corners android:radius="5dp"/>
                    <solid android:color="@color/ksw_md_solid_disable"/>
                </shape>
            </item>
        </layer-list>
    </item>
</selector>
